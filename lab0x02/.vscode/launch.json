    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch key",
            "type": "cppdbg",
            "request": "launch",
            "program": "/lab2/find_my_key/key",
            "stopAtEntry": true,
            "cwd": "/lab2/find_my_key",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}